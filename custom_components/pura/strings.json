          "slot_2": "Slot 2"
        }
      }
    },
    "sensor": {
      "active_fragrance": {
        "name": "Active fragrance",
        "state": {
          "none": "None"
        }
      },
      "controller": {
        "name": "Controller",
        "state": {
          "away": "Away",
          "default": "Default",
          "schedule": "Schedule",
          "timer": "Timer"
        }
      },
      "timer": { "name": "Timer" }
    }
  }
